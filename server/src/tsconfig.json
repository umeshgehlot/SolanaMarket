{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../dist",
    "rootDir": "./",
    "resolveJsonModule": true,
    "noImplicitAny": false
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
